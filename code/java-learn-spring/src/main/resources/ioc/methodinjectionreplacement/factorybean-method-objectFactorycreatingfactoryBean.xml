<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="newsBean" class="caojx.learn.ioc.methodinjectionreplacement.FXNewsBean" scope="prototype"></bean>

    <bean id="newsBeanFactory" class="org.springframework.beans.factory.config.ObjectFactoryCreatingFactoryBean">
        <property name="targetBeanName">
            <idref bean="newsBean"/>
        </property>
    </bean>

    <bean id="mockPersister" class="caojx.learn.ioc.methodinjectionreplacement.MockNewsPersisterObjectFactoryCreatingFactoryBean">
        <property name="newsBeanFactory" ref="newsBeanFactory"></property>
    </bean>


</beans>